# Copy to .env and fill in your RDS MySQL details.
# .env is gitignored; do not commit credentials.
# Use LB_* names to avoid Liquibase 5.x reserved LIQUIBASE_* env var handling.

# RDS MySQL source (reference) database — used for generate-changelog and as reference for diff
LB_SOURCE_URL=jdbc:mysql://your-rds-endpoint.region.rds.amazonaws.com:3306/source_db

# RDS MySQL target database — used for update and diff target
LB_TARGET_URL=jdbc:mysql://your-rds-endpoint.region.rds.amazonaws.com:3306/target_db

# RDS credentials (same or different users for source/target)
LB_USERNAME=your_user
LB_PASSWORD=your_password
